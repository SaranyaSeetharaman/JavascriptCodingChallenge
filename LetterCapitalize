function LetterCapitalize(str) { 
str=str.split("");
var finalString="";
for(var i=0;i<str.length;i++)
{
    if(i===0)
    {
         str[i]=str[i].toUpperCase();
    } else if(str[i]==" ")
    {
        str[i+1]=str[i+1].toUpperCase();
    }
}
  return str.join("");         
}
LetterCapitalize(readline());
